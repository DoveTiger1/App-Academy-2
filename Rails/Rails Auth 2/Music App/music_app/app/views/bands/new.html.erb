<h1>Submit a new band!</h1>

<form action="<%= bands_url %>" method='post'>

    <%= auth_token %>

    <label for="name">Band name:</label>
    <input type="text" name="band[name]" id="name">

    <input type="submit" value="Input Band!">

</form>

<br>
<a href="<%= bands_url %>"><button>Cancel</button></a>